{
  "name": "users",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev:build": "webpack --mode development --colors --watch --progress",
    "dev:start": "nodemon --watch dist --exec 'node dist/server.js'",
    "dev": "npm-run-all --parallel dev:*",
    "build": "rm -rf ./dist && webpack --mode production --colors --progress",
    "dockerize": "docker-compose up --build -d",
    "start": "nodemon app/dist/server.js"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "jest": "^22.4.3",
    "nodemon": "^1.17.4",
    "npm-run-all": "^4.1.3",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3"
  },
  "dependencies": {
    "apollo-server-koa": "^1.3.6",
    "bcryptjs": "^2.4.3",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.1",
    "knex": "^0.14.6",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.0",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "pg": "^7.4.3",
    "webpack-node-externals": "^1.7.2"
  }
}
